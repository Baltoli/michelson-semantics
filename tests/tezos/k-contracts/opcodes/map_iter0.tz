$contract { parameter (map (int :k) (int :e));
storage (pair (int :k) (int :e));
code { CAR; PUSH @acc_e (int :e) 0; PUSH @acc_k (int :k) 0; PAIR % %r; SWAP;
       ITER
         { DIP {DUP; CAR; DIP{CDR}}; DUP; 
           DIP{CAR; ADD}; SWAP; DIP{CDR; ADD}; PAIR % %r };
       NIL operation; PAIR}
 } ;
$param { Elt 0 100 ; Elt 2 100 } ;
$storage (Pair 0 0) ;
$ExpectedOutput (Pair 2 200) ;

